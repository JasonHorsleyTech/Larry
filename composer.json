{
  "name": "larry/larry",
  "description": "The siri style GPT assistant for laravel stacks.",
  "autoload": {
    "psr-4": {
      "Larry\\Larry\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Jason Horsley",
      "email": "Jason@JasonHorsley.tech"
    }
  ],
  "require": {
    "openai-php/client": "^0.6.2",
    "symfony/http-client": "^6.3",
    "nyholm/psr7": "^1.8"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Larry\\Larry\\LarryServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
